local Loops = {}

game:GetService("Players").LocalPlayer.Chatted:Connect(function(Message)
if Message == ".troll" then
Loops.spam = true
repeat wait()
game.Players:Chat(":money_mouth:")
until not Loops.spam
end
end)

game:GetService("Players").LocalPlayer.Chatted:Connect(function(Message)
if Message == ".stroll" then
Loops.spam = false
end
end)
